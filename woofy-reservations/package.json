{
  "name": "woofy_reservations",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": "12.13.0"
  },
  "scripts": {
    "start": "sudo ./scripts/preflight.sh && if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "sudo node ./tests/get_reservations.js"
  },
  "owner_name": "",
  "license": "MIT",
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.6.2",
    "@mapbox/mapbox-gl-geocoder": "^4.5.1",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@stardust-ui/docs-components": "^0.40.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^4.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mapbox-gl": "^1.6.1",
    "mocha": "^7.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.8.9",
    "node-open-geocoder": "^4.0.1",
    "nodemon": "^1.18.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "reactjs-popup": "^1.5.0",
    "semantic-ui-calendar-react": "^0.15.3",
    "semantic-ui-react": "^0.88.2",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "eslint-plugin-react-hooks": "^2.3.0"
  }
}
